  "scripts": {
    "build": "yarn build:js && yarn build:site",
    "build:js": "webpack --mode production",
    "build:js:dev": "webpack --mode development",
    "build:site": "hugo --minify --environment production",
    "clean": "rimraf public themes/sites/assets/js",
    "dev": "concurrently -n site,js \"yarn dev:js\" \"yarn dev:site\"",
    "dev:js": "webpack --watch --mode development",
    "dev:site": "hugo server"
  },
  "dependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "babel-loader": "^8.0.5",
    "bootstrap": "^4.3.1",
    "concurrently": "^4.1.0",
    "jquery": "^3.3.1",
    "popper.js": "^1.14.7",
    "rimraf": "^2.6.3",
    "webpack": "^4.29.3",
    "webpack-cli": "^3.2.3",
    "webpack-pwa-manifest": "^4.0.0",
    "yaml": "^1.3.2"
  }
}
